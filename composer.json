{
    "name": "rafni/laravel-toolkit",
    "description": "Some tools for Laravel development, only commands and helpers to streamline developments",
    "license": "MIT",    
    "authors": [
        {
            "name": "Alberto Rafael",
            "email": "rafnicoder@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravelcollective/html": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Rafni\\LaravelToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rafni\\LaravelToolkit\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist"
    }
}
